#include <iostream>
using namespace std;

typedef long long ll;

int N, M;

int info[100001][11];
ll dp[100001][11] = {0,};


ll dfs(int prev, int pre){
	//prevë‚ 
	if(prev >=N){
		return 0;		
	}
	
	ll ret = 0;
	if(dp[prev][pre] == -1){
		for(int i=0; i<M; i++){
			if(pre == i){
				ret = (ll)info[prev][i]/2 + dfs(prev + 1, i);
				dp[prev][pre] = ret < dp[prev][pre] ? dp[prev][pre] : ret;
			}else{
				ret = (ll)info[prev][i] + dfs(prev+1, i);
				dp[prev][pre] = ret < dp[prev][pre] ? dp[prev][pre] : ret;
			}
		}
	}
	
	return dp[prev][pre];
}


int main() {
	cin >> N >> M;
	for(int i=0 ; i<M; i++){
		for(int j=0; j<N; j++){
			cin >> info[j][i];
			dp[j][i] = -1;
		}
	}
	
	ll ans = 0;
	for(int i=0; i<M; i++){
		ll ret = info[0][i] + dfs(1, i);
		ans = ans < ret ? ret : ans;
	}
	cout << ans;
	
	return 0;
}
